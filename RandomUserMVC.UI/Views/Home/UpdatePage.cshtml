        <!-- Location  -->
        <div class="container">

            <div class="row">
                <div style="background-color:black;height:50px;font: bold 20px tahoma;padding-top:15px" class="col-md-6">
                    <p>Location</p>
                </div>
                <div style="background-color:black;height:50px;" class="col-md-6">
                    <i id="locationButton" onclick="myFunction(this)" class="fas fa-chevron-down"></i>
                </div>
            </div>

            <div id="locationDiv" class="row slideUpDiv">
                <div class="divForm">
                    <form action="/Home/UpdateLocation" method="post">
                        <input type="hidden" name="Id" value="@Model.Id">

                        <div class="form-row">
                            <div class="container">
                                <div class="container">
                                    <div class="form-group col-md-4">

                                        <label for="city">City</label>
                                        <input type="text" class="form-control" name="city" value="@Model.city" id="city">
                                    </div>

                                    <div class="form-group col-md-4">

                                        <label for="state">State</label>
                                        <input type="text" class="form-control" name="state" value="@Model.state" id="state">

                                    </div>

                                    <div class="form-group col-md-4">

                                        <label for="country">Country</label>
                                        <input type="text" class="form-control" name="country" value="@Model.country" id="country">

                                    </div>
                                </div>
                                <div class="container">
                                    <div class="form-group col-md-4">

                                        <label for="postcode">Postcode</label>
                                        <input type="text" class="form-control" name="postcode" value="@Model.postcode" id="postcode">

                                    </div>

                                </div>
                                
                            </div>
                            <div style="border-top: solid; border-width:1px; border-color:gainsboro">
                            <button type="submit" class="btn btn-primary">Update</button>
                        </div>
                        </div>
                    </form>
                    </div>
                </div>
            </div>
        <hr />

        <button id="allUpdateTable" type="submit" class="btn btn-info" style="font-size:large">All Table Update</button>

    </div>

        <!-- AllUpdatesTable -->


        <script type="text/javascript">

            $(document).ready(function () {
                $("#allUpdateTable").on("click", function () {
                    var model = {
                        Id: document.getElementById('userId').value,
                        title: document.getElementById('title').value,
                        first: document.getElementById('first').value,
                        last: document.getElementById('last').value,
                        number: document.getElementById('number').value,
                        streetName: document.getElementById('streetName').value,
                        gender: document.getElementById('gender').value,
                        email: document.getElementById('email').value,
                        phone: document.getElementById('phone').value,
                        cell: document.getElementById('cell').value,
                        nat: document.getElementById('nat').value,
                        uuid: document.getElementById('uuid').value,
                        username: document.getElementById('username').value,
                        password: document.getElementById('password').value,
                        salt: document.getElementById('salt').value,
                        md5: document.getElementById('md5').value,
                        sha1: document.getElementById('sha1').value,
                        sha256: document.getElementById('sha256').value,
                        latitude: document.getElementById('latitude').value,
                        longitude: document.getElementById('longitude').value,
                        name: document.getElementById('name').value,
                        value: document.getElementById('value').value,
                        offset: document.getElementById('offset').value,
                        description: document.getElementById('description').value,
                        large: document.getElementById('large').value,
                        medium: document.getElementById('medium').value,
                        thumbnail: document.getElementById('thumbnail').value,
                        city: document.getElementById('city').value,
                        state: document.getElementById('state').value,
                        country: document.getElementById('country').value,
                        postcode: document.getElementById('postcode').value,
                    }
                    $.ajax({
                        url: '/Home/UpdatePage',
                        type: 'POST',
                        data: model,
                        success: function () {
                            swal("Good job!", "All Table Was Update!", "success")
                        }
                    });
                });
            });

        </script>



