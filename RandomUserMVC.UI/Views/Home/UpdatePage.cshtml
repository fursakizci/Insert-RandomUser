@model Model.AllProperties
@{
    ViewBag.Title = "UpdatePage";
    Layout = "~/Views/Shared/_Layout.cshtml";
  
}
<html>
<head>
    <style>
        p {
            font: bold 20px tahoma;
            color: gray;
        }

            p:hover {
                font: bold 20px tahoma;
                color: white;
            }
        i {
            font-size: xx-large;
            color: gray;
            float: right;
            margin-top: 10px;
        }
            i:hover {
                font-size: xx-large;
                color: white;
                float: right;
                
            }

        .btn-dark {
            background-color: black;
            font-size: x-large;
            color: gray;
            float: right
        }

            .btn-dark:hover {
                background-color: black;
                font-size: xx-large;
                color: white;
                float: right
            }

        .top-header {
            background-color: black;
        }

        .btn-primary {
            background-color: forestgreen;
            float: right;
            margin-right: 10px;
            margin-top: 5px;
            margin-bottom: 5px
        }
        .divForm {
            margin-top:15px; 
            margin-bottom:15px
        }
    </style>


    <script>
        function myFunction(x) {
            x.classList.toggle("fa-chevron-up");
        }

       
    </script>

    <script src="https://kit.fontawesome.com/4dd69257fd.js" crossorigin="anonymous"></script>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script type="text/javascript" language="javascript">

        function isNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : event.keyCode;
            if (charCode > 31 && (charCode < 48 || charCode > 57))
                return false;
            return true;
        }
    </script>
</head>
<body>
    <p style="font-size:10px;"></p>
    <div class="container" style="margin-top:30px">

        <!-- Name -->

        <div class="container">
            <div style="border-bottom: solid;border-width:1px;border-color:gainsboro" class="top-header row">
                <div style="background-color:black;height:50px;padding-top:15px" class="top-header col-md-6">
                     <p>Name</p>
                </div>
                <div id="dene" style="background-color:black;height:50px;" class="top-header col-md-6">
                    <i id="nameButton" onclick="myFunction(this)" class="fas fa-chevron-down"></i>
                </div>
            </div>
            
            <div id="nameDiv" class="row slideUpDiv">
                <div class="divForm" >
                    <form action="/Home/UpdateName" method="post">
                        <input id="userId" type="hidden" name="Id" value="@Model.Id" />

                        <div class="form-row">
                            <div class="container">
                                <div class="form-group col-md-4">
                                    <div class="form-group">

                                        <label for="title">Title</label>
                                        <input type="text" class="form-control" name="title" value="@Model.title" id="title">

                                    </div>
                                </div>
                                <div class="form-group col-md-4">
                                    <div class="form-group">

                                        <label for="first">First</label>
                                        <input type="text" class="form-control" name="first" value="@Model.first" id="first">

                                    </div>
                                </div>
                                <div class="form-group col-md-4">
                                    <div class="form-group">

                                        <label for="first">Last</label>
                                        <input type="text" class="form-control" name="last" value="@Model.last" id="last">

                                    </div>
                                </div>

                            </div>
                            <div style="border-top: solid; border-width:1px; border-color:gainsboro">
                                <button type="submit" class="btn btn-primary">Update</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>

        <hr />

        <!-- Street -->
