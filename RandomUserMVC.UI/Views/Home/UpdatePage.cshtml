                            <div class="container">
                                <div class="form-group col-md-6">
                                    <div class="form-group">

                                        <label for="number">Number</label>
                                        <input type="text" class="form-control" onkeypress="return isNumberKey(event)" name="number" value="@Model.number" id="number">

                                    </div>
                                </div>

